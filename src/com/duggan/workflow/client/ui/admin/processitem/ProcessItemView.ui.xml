<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="tr">
						<g:HTMLPanel styleName="center td">
							<label>
								<input type="checkbox"/>
							</label>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnName"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnDocTypes"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td hide">
						    <span ui:field="spnFileName" class="ellipsis" style="width:61%"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <g:Anchor ui:field="aImageName"></g:Anchor>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnProcessId" class="ellipsis" style="width:71%"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnStatus"></span>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="td">
						    <span ui:field="spnLastModified"></span>
						</g:HTMLPanel>
						
						<g:HTMLPanel styleName="td">
									<g:Anchor ui:field="aActivate" styleName="btn btn-success" title="Activate Process">
										<i class="icon-play"></i>
									</g:Anchor>
									<g:Anchor ui:field="aDeactivate" styleName="btn btn-inverse" title="De-activate Process">
										<i class="icon-minus-sign"></i>
									</g:Anchor>
								
									<g:Anchor ui:field="aRefresh" styleName="btn btn-info" title="Refresh Process">
										<i class="icon-undo"></i>
									</g:Anchor>
									<g:Anchor styleName="btn btn-warning" ui:field="aEdit">
										<i class="icon-pencil helper-14"></i>
									</g:Anchor>
									<g:Anchor styleName="btn btn-danger" ui:field="aDelete">
										<i class="icon-trash"></i>
									</g:Anchor>
						</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>
