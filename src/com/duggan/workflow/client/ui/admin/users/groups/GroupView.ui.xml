<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
	
	<g:HTMLPanel styleName="tr">
			<g:HTMLPanel styleName="td">
				<label>
					<input type="checkbox"/>
				</label>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelFirstName" styleName="td">HOD_Dev</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelLastName" styleName="td">Head of Department</g:HTMLPanel>
			<g:HTMLPanel styleName="td">
				<div class="action-commands">										
					<g:Anchor styleName="green" ui:field="aEdit">
						<i class="icon-pencil helper-14"></i>
					</g:Anchor>
					<g:Anchor styleName="red" ui:field="aDelete">
						<i class="icon-trash"></i>
					</g:Anchor>
				</div>
		</g:HTMLPanel>
    </g:HTMLPanel>
   
</ui:UiBinder>
